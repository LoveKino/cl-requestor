{
  "name": "cl-requestor",
  "version": "0.0.2",
  "description": "Http/Https Requestor which Provide Promise and Common Handler Interface.",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/lib/**/*",
    "cover": "istanbul cover _mocha test/lib/**/*"
  },
  "author": "ddchen",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.3.14"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "cl-spawner": "0.0.1",
    "del": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "mocha": "^2.3.4",
    "run-sequence": "^1.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:LoveKino/cl-requestor.git"
  },
  "eslintConfig": {
    "globals": {
      "describe": true,
      "it": true
    },
    "parser": "babel-eslint",
    "rules": {
      "indent": [
        2,
        4
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-trailing-spaces": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended"
  }
}
